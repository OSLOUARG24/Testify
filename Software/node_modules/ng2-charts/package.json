    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ng2-charts.mjs",
      "esm": "./esm2022/ng2-charts.mjs",
      "default": "./fesm2022/ng2-charts.mjs"
    }
